{
	"name": "Infovore",
	"image": "mcr.microsoft.com/devcontainers/go:1.23-bookworm",
	"runArgs": [
		"--init"
	],
	"forwardPorts": [
		8080
	],
	"portsAttributes": {
		"8080": {
			"label": "Infovore",
			"onAutoForward": "notify"
		}
	},
	"postCreateCommand": "go mod download",
	"customizations": {
		"vscode": {
			"settings": {
				"go.toolsManagement.autoUpdate": true,
				"terminal.integrated.defaultProfile.linux": "bash"
			},
			"extensions": [
				"golang.go"
			]
		}
	}
}